{
  "name": "doutor",
  "version": "0.3.9",
  "type": "module",
  "description": "CLI modular para análise, diagnóstico e manutenção de projetos JavaScript/TypeScript",
  "author": "Italo C Lopes",
  "license": "MIT",
  "bin": {
    "doutor": "./dist/bin/index.js"
  },
  "exports": {
    ".": "./dist/bin/index.js"
  },
  "engines": {
    "node": ">=24.12.0"
  },
  "scripts": {
    "prebuild": "rimraf dist bin",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "test": "vitest run --passWithNoTests",
    "perf:baseline": "doutor perf snapshot --baseline || true",
    "perf:compare": "doutor perf compare",
    "perf:gate": "doutor perf compare --threshold 0",
    "postbuild": "node scripts/copy-artifacts.mjs",
    "prepare": "npm run build",
    "start": "doutor",
    "diagnosticar": "doutor diagnosticar",
    "diagnosticar:json": "doutor diagnosticar --json",
    "formatar": "doutor formatar",
    "formatar:write": "doutor formatar --write",
    "reestruturar": "doutor reestruturar",
    "podar": "doutor podar",
    "fix-types": "doutor fix-types",
    "name": "doutor names",
    "rename": "doutor renames",
    "scan": "doutor licencas scan",
    "scan:root": "doutor licencas scan --root .",
    "notices": "doutor licencas notices generate",
    "adn": "doutor licencas disclaimer add",
    "verify": "doutor licencas disclaimer verify",
    "guardian": "doutor guardian",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint --ext .ts src",
    "validate-aliases": "npm run typecheck && echo 'Aliases válidos!'",
    "licenses:list": "license-checker-rseidelsohn --production --csv",
    "licenses:json": "mkdir -p .doutor && license-checker-rseidelsohn --production --json > .doutor/licenses.json",
    "licenses:auditar": "node scripts/auditar-licencas.mjs",
    "licenses:notice": "node scripts/generate-notices.mjs",
    "headers:spdx": "node scripts/add-spdx-headers.mjs",
    "branch:protect:check": "node scripts/config-branch-protection.mjs check",
    "branch:protect": "node scripts/config-branch-protection.mjs apply",
    "md:fix-fences": "node scripts/fix-md-fences.mjs",
    "md:add-disclaimer": "node scripts/add-disclaimer-md.js",
    "md:verify-disclaimer": "node scripts/verificar-disclaimer-md.js",
    "sync-aliases": "node scripts/sync-aliases.mjs",
    "security:deps": "npm audit",
    "security:lint": "eslint src --ext .ts",
    "md:scan": "doutor diagnosticar --include '**/*.md'"
  },
  "keywords": [
    "cli",
    "code-analysis",
    "linter",
    "static-analysis",
    "typescript",
    "javascript",
    "developer-tools",
    "code-quality",
    "architecture",
    "refactoring",
    "monorepo",
    "project-analysis",
    "technical-debt",
    "devtools",
    "maintenance",
    "diagnostics",
    "code-health"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/md-555/doutor.git"
  },
  "bugs": {
    "url": "https://github.com/md-555/doutor/issues"
  },
  "homepage": "https://github.com/md-555/doutor#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE*",
    "THIRD-PARTY-NOTICES.txt",
    "scripts/check-node-version.js"
  ],
  "dependencies": {
    "@babel/parser": "^7.29.0",
    "@babel/traverse": "^7.29.0",
    "@babel/types": "^7.29.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "css-tree": "^3.1.0",
    "fast-xml-parser": "^5.3.6",
    "htmlparser2": "^10.1.0",
    "java-parser": "^3.0.1",
    "micromatch": "^4.0.8",
    "ora": "^9.3.0",
    "p-limit": "^7.3.0",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/micromatch": "^4.0.10",
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitest/coverage-istanbul": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "doutor": "file:doutor-0.3.9.tgz",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.4.1",
    "husky": "^9.1.7",
    "license-checker-rseidelsohn": "latest",
    "lint-staged": "^16.2.7",
    "rimraf": "^6.1.3",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "lodash": "4.17.23",
    "lodash-es": "4.17.23"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  }
}